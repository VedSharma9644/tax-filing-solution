{
  "expo": {
    "name": "The Growwell Tax",
    "slug": "TheGrowWellTax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.creayaa.thegrowwell",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.creayaa.thegrowwellios",
      "googleServicesFile": "./GoogleService-Info.plist",
      "usesAppleSignIn": true,
      "buildNumber": "6",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.693306869303-qmb0bk8040v91rd8o5lhpcp8mquia2ek"
            ]
          }
        ],
        "NSPhotoLibraryUsageDescription": "We need access to your photo library so you can choose a profile picture.",
        "NSPhotoLibraryAddUsageDescription": "We store your chosen photo as your profile picture.",
        "LSApplicationQueriesSchemes": [
          "googlegmail",
          "googleplus",
          "googlechromes"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.creayaa.thegrowwell",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "Tax Application Updates"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "efcd0532-f8ed-47ab-95a8-50f7765d984e"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      [
        "expo-apple-authentication",
        {
          "appleTeamId": "AW5J6TS678"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1",
            "useFrameworks": "static"
          }
        }
      ],
      "./plugins/withModularHeaders"
    ],
    "owner": ""
  }
}
